{
  "name": "amex-sync",
  "version": "1.0.0",
  "description": "AmEx statement importer with YNAB sync",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npx tsc",
    "build:all": "npm run build && npm --prefix web run build",
    "release": "npx gulp",
    "clean": "npx rimraf out dist docs && npm --prefix web run clean",
    "docs": "npx typedoc --out docs -theme minimal src",
    "lint": "npx eslint src && npm --prefix web run lint",
    "test": "npx ts-mocha spec/*",
    "test:all": "npm test && npm --prefix web run test",
    "start": "node dist/server/index.js",
    "dev": "npx ts-node src/server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm --prefix web run dev -- --host\"",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:up:prod": "docker-compose -f docker-compose.prod.yml up",
    "docker:down:prod": "docker-compose -f docker-compose.prod.yml down"
  },
  "author": {
    "name": "Sathyajith Bhat",
    "email": "sathya@sathyasays.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "better-sqlite3": "^12.5.0",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "multer": "^2.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/chai": "^5.2.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mocha": "^10.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^18.11.3",
    "@types/supertest": "^6.0.3",
    "chai": "^6.2.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "mocha": "^11.7.5",
    "supertest": "^7.1.4",
    "ts-mocha": "^11.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
